            <Button Width="60" Height="60" Content="6" Margin="0,0,20,0"/>
            <Button Width="60" Height="60" Content="7" Margin="0,0,20,0"/>
            <Button Width="60" Height="60" Content="8" Margin="0,0,20,0"/>
            <Button Width="60" Height="60" Content="9" Margin="0,0,20,0"/>
            <Button Width="60" Height="60" Content="10" Margin="0,0,20,0"/>
            <Button Width="60" Height="60" Content="11" Margin="0,0,20,0"/>
            <Button Width="60" Height="60" Content="12"/>
